            <doc title="402550">The Parent Pid of the collection was not found.</doc>
            <doc title="402551">The collection creation failed.</doc>
            <doc title="702266">Collection order must be greater than or equal to 0.</doc>
            <representation mediaType="application/xml">
              <doc title="Collection">rest_collection.xsd</doc>
            </representation>
            <representation mediaType="application/json">
              <doc title="Collection">rest_collection.xsd</doc>
            </representation>
          </response>
        </method>
