            <param name="q" style="query" default="" type="xs:string">
              <doc>Search query. Optional.  Does not work with levels parameter. Searching for text in library, collection name, external system or external ID. Multiple search terms may be combined with AND only.  For example q=external_system~x%20AND%20external_id~y.</doc>
            </param>
          </request>
          <response>
            <doc title="60106">Invalid query format.</doc>
            <doc title="402469">The library query was not valid.</doc>
            <representation mediaType="application/xml">
              <doc title="Collections">rest_collections.xsd</doc>
            </representation>
            <representation mediaType="application/json">
              <doc title="Collections">rest_collections.xsd</doc>
            </representation>
          </response>
        </method>
